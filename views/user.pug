
extends layout

block layout-content
  div.View.UserView
    h1.Banner Comrade #{userProfile.nickname}
    div.Message
      div.Title
        h3 Known information of Prisoner: 
        h1 #{userProfile.nickname}
      pre.Details=JSON.stringify(userProfile, null, 2)
    div.NavButtons
      a(href="/logout")
        div.NavButton log out
      a(href="/rooms")
        div.NavButton rooms

